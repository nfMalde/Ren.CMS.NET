 


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>
    @{
        Ren.CMS.CORE.Helper.TitleHelper.TitleHelper Title = new Ren.CMS.CORE.Helper.TitleHelper.TitleHelper();
        Ren.CMS.CORE.SettingsHelper.GlobalSettingsHelper GSet = new Ren.CMS.CORE.SettingsHelper.GlobalSettingsHelper();
        bool _addthisEnabled = false;
      
            bool.TryParse(GSet.Read("GLOBAL_ADDTHIS_ENABLED"), out _addthisEnabled);
        }
    
    
    @Title.CombineTitle(ViewBag.Title)</title>
        <link href="@Url.Content("~/Content/nftheme/css/Layout.css")" rel="stylesheet" type="text/css" />
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/nftheme/bootstrap/css/bootstrap.css")" />
        
        
       


</head>
<body>
<script type="text/javascript" src="@Url.Content("~/Scripts/nftheme/lib/jquery.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/nftheme/lib/jquery-ui.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Content/nftheme/bootstrap/js/bootstrap.js")"></script> 


 <div class="layout-container container clearfix">
     <div class="layout-header">
     <div class="layout-header-top">

         <div class="layout-header-logo">

             <h2>Ren.CMS.NET</h2>

         </div>
         <div class="layout-header-right">
          <ul class="breadcrumb">
              <li class="pull-left layout-header-user-avatar">

<img class="layout-header-user-avatar" src="">
            

</li>
              <li class="active span3 welcome-user">   
              <strong class="">
                  Willkommen Malte!
             </strong>
                  <span class="divider"></span>
              </li>
              <li>
                  <i class="icon-user"></i>
                  @Html.ActionLink("Mein Account", "Index", "Account")
                  <span class="divider"></span>
              </li>
              <li>
                  <i class="icon-star"></i>
                  @Html.ActionLink("Freunde", "Friends", "Account")
                  <span class="divider"></span>
              </li>
              <li>
                  <i class="icon-off"></i>
                  @Html.ActionLink("Abmelden", "LogOff", "Account")
                   <span class="divider"></span>
              </li>

          </ul>



         </div>

     </div>


<nav>
<div class="navbar navbar-inverse">
  <div class="navbar-inner">
  
    <ul class="nav">
      <li><a href="/Home"><i class="icon-home icon-white"></i> Home</a></li>
      <li><a href="/Blog/News"><i class="icon-globe icon-white"></i> News</a></li>
      <li><a href="/Blog/Article"><i class="icon-signal icon-white"></i> Spieletests</a></li>
      <li><a href="/Forum"><i class="icon-list-alt icon-white"></i> Forum</a></li>
      <li><a href="/Games"><i class="icon-th-large icon-white"></i> Games</a></li>
      
      <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <i class="icon-play icon-white"></i>
              Gamernetzwerk
                    <b class="caret"></b>
          </a>
    

            <ul class="dropdown-menu gamersubmenu">
            
                <li>
                    <a href="#" title="Finde Gamer aus deiner Umgebung oder deinem Spiel"><i class="icon-search"></i> Gamersuche</a>

                </li>
                <li>
                    <a href="#" title="Suche andere Gamer zum Spielen"><i class="icon-volume-up"></i> Gamertreff</a>

                </li>
                <li><a href="#" title="Erstelle eigene Fun-Turniere oder trete einem bei"><i class="icon-list"></i> Community Ladders</a></li>
                <li><a href="#" title="Trete einem Clan bei oder registriere deinen."><i class="icon-cog"></i> Clans</a></li>
            </ul>
      </li>

     
    </ul>
    <form class="navbar-search pull-right">
        <input type="text" class="search-query" placeholder="Search">
    </form>
  </div>
</div>
</nav>


     </div>

<div class="layout-body">
   
    @RenderBody()
 

</div>


    <div style="padding:10px;clear: both;">
       <div class=" clearfix">
        <p class="pull-left">&copy;2013 NetworkFreaks.de - powered by Ren.CMS</p>

        <p class="pull-right"><a href="http://www.networkfreaks.de/Impressum.aspx">Impressum</a></p>
        <div class="alert pull-left">
 
  <strong>Achtung!!!</strong> Dies ist keine Produktiv genutzte Webseite. Hierbei handelt es sich um eine Entwicklungs-Testumgebung.
            Keine Haftung für Links, Bilder dritter, Texte oder Inhalte dritter und ähnlichem.

</div>
    </div>
        </div>
 </div>

  <script>
      $(function () {

          $('.gamersubmenu li a').tooltip({ placement: 'right', container: 'body' });


      });

  </script>
    @Html.RenderScriptFiles()


</body>
</html>
